// Renvoie la matrice d'Ehrenfest de taille 2N+1
ehrenfest(N) fonction
  local j,m;
  m:=matrix(2N+1,2N+1);
  pour j de 0 jusque 2N-1 faire
    m[j,j+1] := 1-j/(2N);
  fpour;
  pour j de 1 jusque 2N faire
    m[j,j-1] := j/(2N);    
  fpour;
  retourne m;
ffonction:;
,
