checkfrobenius(M)=
{
  my([F,B] = matfrobenius(M,2));
  if(M!=B^-1*F*B, error("matfrobenius:",M));
  F
}
{
  for(i=1,15,
      print(checkfrobenius(matrix(i,i,k,j,k>=j))));
  for(i=1,10,
     print(checkfrobenius(matpascal(i))));
  for(i=1,8,
     print(checkfrobenius(mathilbert(i))));
}
