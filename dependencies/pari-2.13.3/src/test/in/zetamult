v = [zeta(9),Pi^2*zeta(7),Pi^4*zeta(5),Pi^6*zeta(3)];
LD=[2,2,2,2];
{
  for(i=1,4,
    LE=LD; LE[i]=3;
    z = lindep(concat(zetamult(LE),v));
    if (z[1] < 0, z = -z); print(z);
  );
}

